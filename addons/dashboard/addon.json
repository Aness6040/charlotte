    "userstyles": [
        {
            "url": "button.css",
            "matches": ["sc", "xmw", "acamp", "cc"]
        }
    ],
    "credits": [
        {
            "name": "EurekaScratch",
            "link": "https://github.com/EurekaScratch/"
        }
    ],
    "dynamicEnable": true,
    "dynamicDisable": true,
    "required": ["api"]
}
