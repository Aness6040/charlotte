{
    "name": "Dashboard",
    "description": "Add entry to open Charlotte's front-end",
    "tags": "official",
    "enabledByDefault": true,
    "userscripts": [
        {
            "url": "gui-button",
            "matches": ["sc", "xmw", "acamp", "cc"],
            "runAtComplete": true
        },
        {
            "url": "category-button",
