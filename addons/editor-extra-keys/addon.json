{
  "name": "Extra key options",
  "description": "Adds more keys to the \"key () pressed?\" and \"when () key pressed\" block dropdowns, such as enter, dot, comma, and more. These keys will work even for users who do not have this addon.",
  "tags": ["sa-ported", "editor", "codeEditor"],
  "credits": [
    {
      "name": "Tacodiva",
      "link": "https://github.com/Tacodiva"
    }
  ],
  "settings": {
    "experimentalKeys": {
      "name": "Show experimental keys",
      "type": "boolean",
      "default": false
    },
    "shiftKeys": {
      "name": "Show Shift keys",
      "type": "boolean",
      "default": false
    }
  },
  "userscripts": [
    {
      "url": "userscript",
      "matches": ["all"]
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "required": ["api"]
}
